#!/bin/bash
cd ~/work

# Signing
croot && git clone https://github.com/Evolution-X/vendor_evolution-priv_keys-template vendor/derp/signing/keys
cd vendor/derp/signing/keys
./keys.sh

source build/envsetup.sh
lunch derp_RMX1901-user
mka derp && wget https://gitlab.com/GustavoMends/go-up/-/raw/master/go-up && chmod +x go-up && ./go-up
